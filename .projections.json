{
    "app/Models/*.php": { "type": "model" },
    "app/*.php": { "alternate": "tests/{}Test.php" },
    "app/Http/Controllers/*.php": { "type": "http" },
    "app/Providers/*.php": { "type": "provider" },
    "config/*.php": { "type": "config" },
    "database/migrations/*.php": { "type": "migration" },
    "resources/views/*.blade.php": { "type": "blade" },
    "resources/assets/js/components/*.vue": { "type": "vue" },
    "routes/web.php": { "type": "route" },
    "routes/api.php": { "type": "api" },
    "routes/cli.php": { "type": "cli" },
    "tests/*Test.php": { "type": "test", "alternate": "app/{}.php" }
}
